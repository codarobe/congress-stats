    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <text><![CDATA[<p>This bill changes the maximum amount of a veteran's home loan of more than $144,000 that is guaranteed by the Department of Veterans Affairs to 25% of the loan amount.</p>]]></text>
          <actionDesc>Introduced in House</actionDesc>
          <actionDate>2017-02-01</actionDate>
