    <summaries>
      <billSummaries>
        <item>
          <versionCode>00</versionCode>
          <updateDate>2017-01-04T05:00:00Z</updateDate>
          <actionDate>2017-01-04</actionDate>
          <text><![CDATA[<p><b>Puerto Rico Medicare Part B Equality Act</b></p> <p>This bill amends title XVIII (Medicare) of the Social Security Act to automatically enroll eligible residents of Puerto Rico in Medicare's medical insurance program. Under current law, automatic enrollment applies to eligible residents of the United States, excluding Puerto Rico. </p>]]></text>
          <actionDesc>Introduced in House</actionDesc>
          <lastSummaryUpdateDate>2017-04-06T18:57:00Z</lastSummaryUpdateDate>
          <name>Introduced in House</name>
        </item>
      </billSummaries>
